{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch FastAPI (Uvicorn)",
      "type": "python",
      "request": "launch",
      "module": "uvicorn",
      "args": [
        "app.main:app",
        "--host",
        "0.0.0.0",
        "--port",
        "9432",
        "--reload"
      ],
      "envFile": "${workspaceFolder}/.env",
      "jinja": true,
      "justMyCode": true
    }
  ]
}
